}

HideTollTD(playerid)
{
    for(new x; x < 26; x++)
    {
        TextDrawHideForPlayer(playerid, TollTD[x]);
    }
    ShowHBETD(playerid);
    ShowServerNameTD(playerid);
    CancelSelectTextDraw(playerid);
}